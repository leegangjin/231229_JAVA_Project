package day13;
class Animal{
	private String name; //이름
	private String category; //종
	
	public Animal() {}
	
	public Animal(String name, String category) {
		this.name=name;
		this.category=category;
	}
	public void printInfo() {
		System.out.println("이름:"+name);
		System.out.println("종류:"+category);
	}
	//울음소리메서드
	public void howl() {
		System.out.println("--+name"+"의 울음소리--");
	}

public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	class howl extends Animal {

		@Override
		public void printInfo() {
			// TODO Auto-generated method stub
			super.printInfo();
		}

		@Override
		public void howl() {
			// TODO Auto-generated method stub
			super.howl();
		}
		
	}
}
//Dog Cat Animal 클래스를 상속
	//printinfo()
	//howl()오버라이드 상속
	
	
	
	
public class 상속02 {

	public static void main(String[] args) {
		/* 오버라이딩 : 부모 클래스에게서 물려받은 메서드를 재정의 하는 것.
		 * -부모 클래스의 메서드와 선언부가 일치해야 한다.
		 *  -접근제한자도 더 넓은 범위는 가능하지만 축소는 안됨
		 */
	//	Dog dog = new Dog();
		
		Animal an = new Animal();
		an.setName("뽀삐");
		an.setCategory("dog");
		an.printInfo();
		Animal an1 = new Animal();
		an1.setName("치치");
		an1.setCategory("cat");
		an.printInfo();
		
	}


}
