package ddddddddd;

import java.util.Arrays;

public class 변수합 {

	public static void main(String[] args) {
		
		int[][] arr = {{10,20,50},{20,30,15},{100,110,120}};	
		int sum=0;
		
		for(int i =0; i < arr.length; i++) {
			for(int j=0; j < arr[i].length; j++) {
				
			}
		}
		for(int[] temp : arr) {
            for(int i : temp) {
                sum +=i;
            }
        }
        System.out.println("2차원 배열의 총 합 : " + sum);
		
		
		

		

	}

}
